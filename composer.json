{
    "name": "vertisanpro/laravel-freekassa",
    "description": "freekassa.ru payments for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "vertisanpro",
            "email": "ya@sanek.dev"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=6.*"
    },
    "autoload": {
        "psr-4": {
            "VertisanPRO\\FreeKassa\\Facades\\": "src/Facades",
            "VertisanPRO\\FreeKassa\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VertisanPRO\\FreeKassa\\FreeKassaServiceProvider"
            ],
            "aliases": {
                "FreeKassa": "VertisanPRO\\FreeKassa\\Facades\\FreeKassa"
            }
        }
    }
}